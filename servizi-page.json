              "isInner": false,
              "settings": {
                "editor": "<p>Un approccio strutturato e rigoroso per la diagnosi e la riabilitazione delle funzioni cognitive.</p>",
                "text_color": "#555555",
                "typography_typography": "custom",
                "typography_font_family": "Inter",
                "typography_font_size": {"unit": "px", "size": 15},
                "typography_font_weight": "300",
